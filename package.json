{
  "name": "@arcana/storage",
  "version": "0.1.3",
  "description": "Upload, Download, Share can be performed using this SDK",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "unpkg": "dist/standalone/storage.umd.js",
  "jsdelivr": "dist/standalone/storage.umd.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "rimraf dist types && npm run build:ts && npm run build:web",
    "build:web": "webpack",
    "build:ts": "tsc --project tsconfig.json",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.js\"  ",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "test": "mocha 'test/**/*.ts' --recursive --timeout 3000000 -R spec --ui bdd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcana-network/storage-sdk.git"
  },
  "keywords": [],
  "author": "Saurav Kanchan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arcana-network/storage/issues"
  },
  "homepage": "https://github.com/arcana-network/storage#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "crypto-browserify": "^3.12.0",
    "inline-environment-variables-webpack-plugin": "^1.2.1",
    "mocha": "^8.4.0",
    "node-polyfill-webpack-plugin": "^1.1.3",
    "prettier": "^2.3.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.2.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@sentry/browser": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "assert": "^2.0.0",
    "axios": "^0.21.4",
    "buffer": "^6.0.3",
    "eth-crypto": "^1.9.0",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.3.1",
    "tus-js-client": "^2.3.0",
    "tweetnacl": "^1.0.3"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
